{
    "version": 2,
    "functions": {
        "api/app.py": {
            "memory": 1024,
            "maxDuration": 10,
            "runtime": "@vercel/python@3.1.0"
        }
    },
    "routes": [
        {
            "src": "/_vercel/insights/script.js",
            "dest": "https://va.vercel-scripts.com/v1/script.js"
        },
        {
            "src": "/_vercel/insights/(.*)",
            "dest": "https://va.vercel-analytics.com/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/app.py"
        }
    ]
}